{
  "name": "girders-elements",
  "version": "1.0.0-beta.0",
  "description": "Girders Elements is an architectural framework that assists with building data-driven apps with React or React Native.",
  "main": "index.js",
  "browser": "lib/index.js",
  "scripts": {
    "test": "cross-env BABEL_ENV=test mocha --recursive --compilers js:babel-core/register --require test/support/utils",
    "coverage": "nyc npm test",
    "coverage-report": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "lint-stats": "eslint . --format node_modules/eslint-stats/byErrorAndWarning.js",
    "clean": "del-cli lib/",
    "build": "babel src --out-dir lib/",
    "prepublish": "npm run clean && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netceteragroup/girders-elements.git"
  },
  "keywords": [
    "react",
    "react-native",
    "redux",
    "redux-elm",
    "redux-saga",
    "netcetera"
  ],
  "author": "netcetera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netceteragroup/girders-elements/issues"
  },
  "homepage": "https://github.com/netceteragroup/girders-elements#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "omniscient": "^4.1.1",
    "ramda": "^0.22.1",
    "redux-saga": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.4",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.2",
    "del-cli": "^0.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.10.1",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-redux-saga": "^0.1.4",
    "eslint-stats": "^0.1.4",
    "mocha": "^2.5.3",
    "nyc": "^8.4.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-redux": "^4.3.0",
    "redux": "^3.0.0"
  }
}
